# Gemfile

# Fuente oficial de gems
source "https://rubygems.org"

# Especifica la versión de Jekyll que quieres usar localmente
gem "jekyll", "~> 4.3"

# Para que GitHub Pages use los mismos gems que en producción
gem "github-pages", group: :jekyll_plugins

# Tema remoto y plugins adicionales
gem "just-the-docs"
gem "jekyll-feed"
gem "jekyll-seo-tag"
